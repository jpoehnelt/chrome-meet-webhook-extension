{
  "name": "@jpoehnelt/chrome-meet-webhook-extension",
  "version": "1.0.0",
  "description": "Trigger a webhook when the microphone or camera is turned on or off in a Google Meet.",
  "keywords": [
    "google",
    "meet",
    "webhook"
  ],
  "homepage": "https://github.com/jpoehnelt/chrome-meet-webhook-extension",
  "bugs": {
    "url": "https://github.com/jpoehnelt/chrome-meet-webhook-extension/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jpoehnelt/chrome-meet-webhook-extension.git"
  },
  "license": "Apache-2.0",
  "author": "Justin Poehnelt",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "prettier *.json *.js src/* e2e/* examples/* --write",
    "lint": "eslint src/*",
    "build": "rollup -c",
    "test": "jest src/*",
    "package": "zip dist/chrome-meet-webhook-extension.zip dist/*.js public/* manifest.json"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime-corejs3": "^7.9.2",
    "@types/chrome": "0.0.104",
    "@types/jest": "^25.2.1",
    "@types/selenium-webdriver": "^4.0.9",
    "@typescript-eslint/eslint-plugin": ">=2.25.0",
    "@typescript-eslint/parser": ">=2.25.0",
    "chromedriver": "^80.0.2",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "rollup": "^2.4.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  }
}
