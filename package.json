  },
  "license": "Apache-2.0",
  "author": "Justin Poehnelt",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "prettier *.json *.js src/* --write",
    "lint": "eslint src/*",
    "build": "rollup -c",
