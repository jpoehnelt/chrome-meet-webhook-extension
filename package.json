    "@typescript-eslint/parser": "^3.10.1",
    "chromedriver": "^97.0.1",
    "core-js": "^3.20.3",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^26.0.0",
    "jest": "^25.5.4",
    "prettier": "^2.5.1",
    "rollup": "^2.66.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "selenium-webdriver": "^4.1.1",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.10"
